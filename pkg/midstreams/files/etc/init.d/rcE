#============================#


for i in /etc/init.d/S??* ;do
    [ ! -f "$i" ] && continue
    case "$i" in
    *.sh)
        # Shell Script
        (
        trap - INT QUIT TSTP
        set stop
        . $i
        )
        ;;
    *)
        # Fork
        $i stop
        ;;
    esac
done
